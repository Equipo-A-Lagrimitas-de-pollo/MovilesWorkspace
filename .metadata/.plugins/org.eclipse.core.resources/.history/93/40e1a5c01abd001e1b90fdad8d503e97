package com.moviles.filter;

import com.fasterxml.jackson.core.filter.TokenFilter;
import com.moviles.filter.DecoratorFilter;

public abstract class BaseDecoratorFilter<ToFilter, Parameter> extends DecoratorFilter<ToFilter, Parameter>{

	protected DecoratorFilter<TokenFilter,?> wrappedDecorator;
	public BaseDecoratorFilter(Parameter parameter, DecoratorFilter<TokenFilter,?> wrappedDecorator) {
		super(parameter);
		this.wrappedDecorator=wrappedDecorator;
	}


}
